generator client {
  provider = "prisma-client-js"
  output   = "../generated/reservations-client"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:root@localhost:5432/reservationdb"
}


model Reservation {
  id                    String   @id @default(uuid())
  propertyId            String
  platform              String
  platformReservationId String
  checkIn               DateTime
  checkOut              DateTime
  guestsCount           Int
  totalPrice            Float
  status                String

}