generator client {
  provider = "prisma-client-js"
  output   = "../generated/auth-client"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:root@localhost:5432/authdb"
}

model User{
  id          Int     @id @default(autoincrement())
  email       String  @unique
  name        String
  password    String
  role        String?
  company_id  String?
}
